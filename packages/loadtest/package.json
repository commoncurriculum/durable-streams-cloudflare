  },
  "dependencies": {
    "@durable-streams/client": "^0.2.1",
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260304.0",
    "@types/node": "^25.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.68.1"
  }
}
