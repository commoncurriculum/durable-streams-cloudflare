{
  "name": "@durable-streams-cloudflare/loadtest",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/run.ts",
    "diagnose": "tsx src/diagnose-cdn.ts",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "format": "oxlint --fix src && oxfmt --write src",
