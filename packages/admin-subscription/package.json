{
  "name": "@durable-streams-cloudflare/admin-subscription",
  "version": "0.2.0",
  "type": "module",
  "license": "MIT",
  "description": "Admin dashboard for durable-streams subscription service on Cloudflare Workers",
  "repository": {
    "type": "git",
